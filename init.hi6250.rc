import /init.hi6250.power.rc
import /init.hi6250.usb.configfs.rc
import /init.hi6250.usb.rc

on early-init
    setprop wifi.interface wlan0
    setprop is_hisi_connectivity_chip 1
    setprop ro.hardware.consumerir "hisi.hi6250"
    setprop persist.service.hdb.enable true

on init
    mkdir /cust
    mkdir /vendor
    mkdir /product
    mkdir /version
    mkdir /splash2
    mkdir /3rdmodem
    mkdir /3rdmodemnvm
    mkdir /3rdmodemnvmbkp
    mkdir /modem_log
    mkdir /mnvm2:0
    mkdir /modem_fw
    mkdir /sec_storage 600 root root
    mkdir /patch_hw 0775 system system

    symlink /sdcard /storage/sdcard0

    symlink /dev/block/platform/hi_mci.0 /dev/block/bootdevice

    write /sys/module/hisi_nve/parameters/nve /dev/block/bootdevice/by-name/nvme

    wait /dev/nve0
    chown root system /dev/nve0
    chmod 0770 /dev/nve0

    chmod 0400 /dev/hisi_teelog

    chown radio system /dev/flp
    chmod 0660 /dev/flp

    mkdir /dev/frz
    mount cgroup none /dev/frz freezer

    chmod 0660 /dev/hwlog_switch
    chown logd system /dev/hwlog_switch

    mount tmpfs none /sys/fs/cgroup mode=0750,uid=0,gid=1000
